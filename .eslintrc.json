{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len":"warn",
        "no-unused-vars":"warn",
        "linebreak-style":"off",
        "no-console":"off"
    }
}
